package com.wipro.app;

class Node {
    int data;
    Node next;
}
class LinkedlistInsertion{
	void insertAtFront(Node head, int newData) {
	    Node newNode = new Node();
	    newNode.data = newData;
	    newNode.next = head;
	    head = newNode;
	}

	void insertAtEnd(Node head, int newData) {
	    Node newNode = new Node();
	    newNode.data = newData;
	    newNode.next = null;

	    if (head == null) {
	        head = newNode;
	        return;
	    }

	    Node last = head;
	    while (last.next != null) {
	        last = last.next;
	    }
	    last.next = newNode;
	}

	void insertAtPosition(Node head, int newData, int position) {
	    Node newNode = new Node();
	    newNode.data = newData;

	    if (position == 1) {
	        newNode.next = head;
	        head = newNode;
	        return;
	    }

	    Node prev = head;
	    for (int i = 1; i < position - 1; i++) {
	        prev = prev.next;
	    }

	    newNode.next = prev.next;
	    prev.next = newNode;
	}

	void deleteByKey(Node head, int key) {
	    Node temp = head;
	    Node prev = null;

	    if (temp != null && temp.data == key) {
	        head = temp.next;
	        return;
	    }

	    while (temp != null && temp.data != key) {
	        prev = temp;
	        temp = temp.next;
	    }

	    if (temp == null) {
	        return; // Key not found
	    }

	    prev.next = temp.next;
	}
}


void printList(Node head) {
    while (head != null) {
        System.out.print(head.data + " ");
        head = head.next;
    }
    System.out.println();
}

public static void main(String[] args) {
  
    LinkedListInsertion list = new LinkedListExample();

    
}